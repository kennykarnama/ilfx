      <fsiRule>
                

var codes = [
    "SI01023001": 0,
    "SI01023002": 0,
    "SI01023003": 0,
    "SI01023004": 0,
    "SI01023005": 0,
    "SI01001000": 0
];

for (code : codes){
    var item = companyValuesByCodeAndCompanyName(code.first, companyName);
    codes[code.first] = item;
}

var result = 0.0;

print(codes);

if (codes["SI01001000"] != 0){
    var ratio = (codes["SI01023001"] + codes["SI01023002"] + codes["SI01023003"] + codes["SI01023004"] + codes["SI01023005"]) * 1.0 / codes["SI01001000"] * 1.0;
    result = ratio;
} else {
    result = 0;
}

print(result);

return result;

                
            </fsiRule>
